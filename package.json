{
  "name": "@aiready/visualizer",
  "version": "0.1.0",
  "description": "Interactive visualization tool for AIReady codebase analysis",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "aiready-visualize": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:web": "cd web && pnpm build",
    "dev:web": "cd web && pnpm dev",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "clean": "rm -rf dist web/dist"
  },
  "keywords": [
    "aiready",
    "visualization",
    "graph",
    "dependencies",
    "codebase",
    "analysis"
  ],
  "author": "AIReady",
  "license": "MIT",
  "dependencies": {
    "@aiready/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}