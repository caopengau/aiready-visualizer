{
  "name": "@aiready/visualizer",
  "version": "0.1.0",
  "description": "Interactive graph visualization for AIReady analysis results",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "aiready-visualize": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./graph": {
      "import": "./dist/graph/index.js",
      "types": "./dist/graph/index.d.ts"
    }
  },
  "files": [
    "dist",
    "web/dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "dev:web": "cd web && vite",
    "dev:watch-report": "node ../scripts/watch-report.js",
    "build": "tsup && cd web && vite build",
    "build:cli": "tsup",
    "build:web": "cd web && vite build",
    "typecheck": "tsc --noEmit && cd web && tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "aiready",
    "visualization",
    "graph",
    "d3",
    "dependency-graph"
  ],
  "author": "Peng Cao <caopengau@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/caopengau/aiready-visualizer.git"
  },
  "dependencies": {
    "@aiready/core": "workspace:*",
    "commander": "^13.0.0"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "@types/node": "^22.10.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}